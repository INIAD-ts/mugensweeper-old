{
  "name": "next-frourio-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "notios": "notios dev",
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 3000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:path": "pathpida -s -w",
    "dev:typecheckClient": "tsc --noEmit -w",
    "dev:typecheckServer": "tsc --noEmit -p server -w",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "next build && next export",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:path": "pathpida -s",
    "lint": "run-p lint:js lint:prettier lint:style",
    "lint:js": "eslint --ext .ts,.tsx,.js --ignore-path .gitignore .",
    "lint:prettier": "prettier --check \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore",
    "lint:style": "stylelint \"**/*.ts{,x}\"",
    "lint:fix": "npm run lint:js -- --fix && prettier --write \"./**/*.{ts,tsx,js}\" --ignore-path .gitignore && npm run lint:style -- --fix",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:dev:createonly": "npm run migrate:dev:createonly --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "seed": "npm run seed --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "task:scrape": "ts-node tasks/scrape.ts",
    "task:code": "ts-node tasks/genAddressCode.ts",
    "pretypecheck": "npm run generate && npm run generate --prefix server",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:updateSnapshot": "vitest -u",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@aspida/swr": "^1.11.0",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.9.3",
    "@mantine/core": "^5.10.0",
    "@mantine/ds": "^5.10.0",
    "@mantine/hooks": "^5.10.0",
    "@mantine/next": "^5.10.0",
    "@mantine/prism": "^5.10.0",
    "@tabler/icons": "^1.119.0",
    "axios": "^0.27.2",
    "canvas": "^2.11.0",
    "firebase": "^9.15.0",
    "jotai": "^1.13.0",
    "konva": "^8.4.2",
    "next": "^13.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.5",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "^0.37.2",
    "@types/node": "^17.0.18",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.22.0",
    "eslint-config-next": "^12.2.5",
    "eslint-config-prettier": "^8.5.0",
    "node-fetch": "2",
    "notios": "^0.4.0",
    "pathpida": "^0.20.1",
    "postcss-jsx": "^0.36.4",
    "postcss-syntax": "^0.36.2",
    "prettier": "^2.8.0",
    "prettier-plugin-organize-imports": "^3.2.1",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^27.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.22.1"
  },
  "engines": {
    "node": "18.x"
  }
}
